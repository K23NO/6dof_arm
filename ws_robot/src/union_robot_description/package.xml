<?xml version="1.0"?>
<package format="3">
  <name>union_robot_description</name>
  <version>0.0.0</version>
  <description>ROS 2 (Humble) description package for the union_robot, generated from Fusion 360 and adapted for RViz2 and Gazebo.</description>

  <maintainer email="syuntoku14@todo.todo">syuntoku14</maintainer>
  <license>TODO</license>

  <!-- Build system -->
  <buildtool_depend>ament_cmake</buildtool_depend>

  <!-- Runtime dependencies for launching and visualization -->
  <exec_depend>xacro</exec_depend>
  <exec_depend>robot_state_publisher</exec_depend>
  <exec_depend>joint_state_publisher</exec_depend>
  <exec_depend>joint_state_publisher_gui</exec_depend>
  <exec_depend>rviz2</exec_depend>
  <exec_depend>gazebo_ros</exec_depend>
  <exec_depend>ament_index_python</exec_depend>
  <exec_depend>launch</exec_depend>
  <exec_depend>launch_ros</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
